{
  "name": "tosingleline",
	"displayName": "To Single Line",
	"description": "Replace all car returns with spaces",
	"publisher": "GuiExtensions",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.61.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/xiillii/ToSingleLineVSCodeExtension"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:tosingleline.execute"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tosingleline.execute",
				"title": "To Single Line"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}
